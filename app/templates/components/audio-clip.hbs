{{wave-surfer
  file=model.track.file
  streamUrl=model.track.streamUrl

  isPlaying=isPlaying
  seekTime=audioSeekTime

  pxPerBeat=pxPerBeat
  audioBpm=audioBpm

  volume=model.volume
  pitch=model.pitch
  tempo=tempo

  didLoadWave='didLoadWave'
}}

{{#each region in regions}}
  {{clip-region
    startBeat=region.startBeat
    endBeat=region.endBeat
    color=region.color
    pxPerBeat=pxPerBeat
  }}
{{/each}}