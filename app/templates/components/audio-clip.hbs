{{wave-surfer
  file=model.track.file
  streamUrl=model.track.streamUrl

  isPlaying=isPlaying
  seekTime=audioSeekTime

  pxPerBeat=pxPerBeat
  audioBpm=audioBpm

  volume=model.volume
  pitch=model.pitch
  tempo=tempo

  didLoadWave='didLoadWave'
}}

{{#each marker in visibleMarkers}}
  {{clip-marker
    marker=marker
    pxPerBeat=pxPerBeat
  }}
{{/each}}