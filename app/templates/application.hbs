<h2 id="title">Welcome to Ember.js</h2>

<div class="main-container">
  {{#arrangement-grid arrangement=arrangement}}
    {{#each row in arrangement.sortedRows}}
      {{#arrangement-row row=row}}
        {{#each clip in row.clips}}
          {{arrangement-clip arrangementClip=clip}}
        {{/each}}
      {{/arrangement-row}}
    {{/each}}
  {{/arrangement-grid}}
</div>


