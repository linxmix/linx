{
  "name": "linx",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "gulp && browserify src/index.js -o static/index.js -g uglifyify",
    "watch": "gulp && beefy ../src/index.js:index.js --cwd static --live",
    "test": "(export NODE_ENV=test; testling -x google-chrome)",
    "develop": "(export NODE_ENV=development; npm run watch)",
    "start": "(export NODE_ENV=production; npm run build)",
    "deploy": "npm run start && ./bin/deploy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/linxmusic/linx.git"
  },
  "author": "",
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/linxmusic/linx/issues"
  },
  "homepage": "https://github.com/linxmusic/linx",
  "browserify": {
    "transform": [
      "envify",
      "reactify",
      "lessify"
    ]
  },
  "testling": {
    "files": "test/**/*.js",
    "harness": "mocha",
    "browsers": [
      "ie/9..latest",
      "firefox/17..nightly",
      "chrome/23..canary",
      "opera/15..next",
      "safari/6..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/4.2"
    ]
  },
  "devDependencies": {
    "chai": "~1.9.0",
    "mocha": "~1.18.0",
    "testling": "^1.6.1",
    "beefy": "^1.1.0"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "jquery": "^2.1.0",
    "backbone-react-component": "^0.6.0",
    "react": "^0.10.0",
    "reactify": "^0.10.0",
    "envify": "^1.2.1",
    "uglifyify": "^2.0.0",
    "vinyl-source-stream": "^0.1.1",
    "browserify": "^3.32.1",
    "lessify": "git://github.com/ahdinosaur/lessify",
    "wavesurfer.js": "0.1.6",
    "semantic": "git://github.com/Semantic-Org/Semantic-UI#0.15.4",
    "debug": "^0.7.4",
    "echojs": "git://github.com/ahdinosaur/echojs",
    "URIjs": "^1.12.1",
    "underscore": "^1.6.0",
    "bluebird": "^1.2.1",
    "browser-keymap": "0.0.1",
    "queue": "^2.2.0",
    "d3": "^3.4.6",
    "jquery-overscroll": "git://github.com/linxmusic/overscroll",
    "react-addons": "^0.9.0",
    "gulp-gh-pages": "^0.2.0",
    "gulp-replace": "^0.3.0"
  }
}
